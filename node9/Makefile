	@${MAKE} --no-print-directory -C . -f ndate.make config=$(ndate_config)
endif

node9:
ifneq (,$(node9_config))
	@echo "==== Building node9 ($(node9_config)) ===="
	@${MAKE} --no-print-directory -C . -f node9.make config=$(node9_config)
endif

libnode9:
ifneq (,$(libnode9_config))
	@echo "==== Building libnode9 ($(libnode9_config)) ===="
	@${MAKE} --no-print-directory -C . -f libnode9.make config=$(libnode9_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f lib9.make clean
	@${MAKE} --no-print-directory -C . -f libbio.make clean
	@${MAKE} --no-print-directory -C . -f libsec.make clean
	@${MAKE} --no-print-directory -C . -f ndate.make clean
	@${MAKE} --no-print-directory -C . -f node9.make clean
	@${MAKE} --no-print-directory -C . -f libnode9.make clean

