PROJECTS := lib9 libbio libsec ndate node9 libnode9

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

lib9:
ifneq (,$(lib9_config))
	@echo "==== Building lib9 ($(lib9_config)) ===="
	@${MAKE} --no-print-directory -C . -f lib9.make config=$(lib9_config)
endif

libbio:
ifneq (,$(libbio_config))
	@echo "==== Building libbio ($(libbio_config)) ===="
	@${MAKE} --no-print-directory -C . -f libbio.make config=$(libbio_config)
endif

libsec:
ifneq (,$(libsec_config))
